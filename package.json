{
  "name": "slask",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/nvandoorn/slask"
  },
  "keywords": [
    "log",
    "viewer"
  ],
  "author": "Nicholas Van Doorn",
  "main": "scripts/start.js",
  "bin": {
    "slask": "scripts/start.js"
  },
  "scripts": {
    "lint": "standard",
    "dev": "node scripts/dev.js",
    "dev-debug": "node debug scripts/dev.js",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "standard && ./node_modules/jest/bin/jest.js --env=jsdom --forceExit=true"
  },
  "dependencies": {
    "async": "^2.1.2",
    "autobind-decorator": "^1.4.0",
    "autoprefixer": "6.7.2",
    "axios": "^0.16.1",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-app": "^2.1.0",
    "babel-runtime": "^6.20.0",
    "blocked": "^1.2.1",
    "body-parser": "~1.15.1",
    "bootstrap": "^3.3.7",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chai": "^3.5.0",
    "chalk": "1.1.3",
    "clone": "^2.0.0",
    "concurrently": "^3.4.0",
    "connect-history-api-fallback": "1.3.0",
    "cookie-parser": "~1.4.3",
    "create-react-class": "^15.5.2",
    "cross-spawn": "4.0.2",
    "css-js-loader": "^0.3.0",
    "css-loader": "0.26.1",
    "dateformat": "^2.0.0",
    "debug": "~2.2.0",
    "decorator": "0.0.1",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "^0.5.1",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "events": "^1.1.1",
    "express": "~4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "filesize": "3.3.0",
    "fs-extra": "^2.0.0",
    "gzip-size": "3.0.0",
    "heap": "^0.2.6",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.3",
    "input-moment": "^0.3.2",
    "jest": "^19.0.2",
    "json-loader": "0.5.4",
    "json-subset": "^1.0.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "morgan": "~1.7.0",
    "object-assign": "4.1.1",
    "postcss-loader": "1.2.2",
    "postcss-nesting": "^2.3.1",
    "process": "^0.11.9",
    "promise": "7.1.1",
    "q": "^1.5.0",
    "react": "^15.5.4",
    "react-bootstrap": "^0.30.8",
    "react-dev-utils": "^0.5.0",
    "react-dom": "^15.4.2",
    "react-nanobar": "^1.0.0",
    "react-spinkit": "^2.1.1",
    "react-test-renderer": "^15.4.2",
    "react-transition-group": "^1.1.3",
    "recursive-readdir": "2.1.0",
    "request": "^2.81.0",
    "serve-favicon": "~2.3.0",
    "spinkit": "^1.2.5",
    "standard": "^9.0.2",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "tail": "^1.2.1",
    "url-loader": "0.5.7",
    "walk": "^2.3.9",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.2",
    "winston": "^2.3.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "build"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
